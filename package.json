  "scripts": {
    "debug": "node inspect dist/a.js",
    "compile": "tsc --build build && gulp compile-client --max_old_space_size=4095 && cp -av resources dist/ && tsc --build src",
    "gulp-compile": "gulp compile --max_old_space_size=4095",
    "prestart": "yarn build",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node_modules/.bin/electron dist/main.js",
    "exe64": "electron-packager . hyper-t --platform linux --arch x64 --out out/ --ignore \"(installer|node_modules/electron.*)\"",
    "rpm64": "electron-installer-redhat --src out/hyper-t-linux-x64/ --arch x86_64 --config rhel_installer.json",
    "clean": "rimraf out && rimraf installer && rimraf dist",
    "installer": "yarn clean && yarn build && yarn exe64 && yarn rpm64",
    "electron-rebuild": "node_modules/.bin/electron-rebuild node_modules/node-pty && node_modules/.bin/electron-rebuild node_modules/vscode-sqlite3 && node_modules/.bin/electron-rebuild node_modules/vscode-ripgrep && node_modules/.bin/electron-rebuild node_modules/native-watchdog"
  },
